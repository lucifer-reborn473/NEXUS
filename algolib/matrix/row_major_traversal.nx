fn rowMajorTraversal(matrix, rows, cols) {
    displayl "Row Major Traversal:";
    for(var i = 0; i < rows; i += 1) {
        var rowStr = "";
        for(var j = 0; j < cols; j += 1) {
            var index = i * cols + j;
            rowStr = rowStr + matrix[index] + " ";
        };
        displayl rowStr;
    };
};
